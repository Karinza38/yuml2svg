{
  "name": "yuml",
  "displayName": "yUML",
  "description": "UML diagramming tool based on the yUML syntax",
  "version": "3.0.0",
  "icon": "images/yUML.png",
  "publisher": "JaimeOlivares",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aduh95/vscode-yuml"
  },
  "bugs": {
    "url": "https://github.com/aduh95/vscode-yuml/issues"
  },
  "homepage":
    "https://github.com/jaime-olivares/vscode-yuml/blob/master/README.md",
  "engines": {
    "node": ">=6.5.0"
  },
  "categories": ["Languages", "Snippets", "Other"],
  "keywords": ["yuml", "uml", "diagrams"],
  "contributes": {
    "languages": [
      {
        "id": "yuml",
        "aliases": ["yUML", "yuml"],
        "extensions": [".yuml"],
        "configuration": "./yuml.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "yuml",
        "scopeName": "source.yuml",
        "path": "./syntaxes/yuml.tmLanguage"
      }
    ],
    "commands": [
      {
        "command": "extension.viewYumlDiagram",
        "title": "View yUML Diagram",
        "icon": {
          "light": "./images/preview.svg",
          "dark": "./images/preview_inverse.svg"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceLangId == yuml",
          "command": "extension.viewYumlDiagram",
          "group": "navigation"
        }
      ]
    },
    "snippets": [
      {
        "language": "yuml",
        "path": "./snippets/yuml.json"
      }
    ]
  },
  "main": "./index.js",
  "dependencies": {
    "viz.js": "^1.7.1"
  },
  "devDependencies": {
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  }
}
