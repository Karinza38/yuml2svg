  "description": "UML diagramming tool based on the yUML syntax",
  "version": "5.0.0-rc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aduh95/yuml2svg"
  },
  "bugs": {
    "url": "https://github.com/aduh95/yuml2svg/issues"
  },
  "engines": {
